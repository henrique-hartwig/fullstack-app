{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "noImplicitAny": false,
    "noEmit": true
  },
  "include": [
    "src",
    "@types"
  ],
  "exclude": [
    "node_modules"
  ]
}
