{
    "include": [
		"src/**/*",
		"test/**/*",
		"@types"
	],
	"compilerOptions": {
		"outDir": "./dist",
		"target": "ESNext",
		"module": "nodenext",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"noImplicitAny": false,
		"noEmit": true
	},
	"exclude": [
		"node_modules"
	]
}